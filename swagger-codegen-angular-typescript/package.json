{
	"name": "swagger-codegen-angular-typescript",
	"displayName": "Swagger Codegen Angular Typescript",
	"description": "Tool that generates Angular typescript models and client code from  a swagger open API specification JSON uri/file.",
	"version": "0.0.1",
	"icon": "images/AngularDocs.png",
	"keywords": [
		"codegen",
		"swagger",
		"typescript",
		"angular",
		"openapi"
	],
	"publisher": "Imaginations Delivered, LLC.",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onCommand:swagger-codegen-angular-typescript.genclient",
		"onCommand:swagger-codegen-angular-typescript.genspec"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "swagger-codegen-angular-typescript.genclient",
				"title": "Generate Client"
			},
			{
				"command": "swagger-codegen-angular-typescript.genspec",
				"title": "Generate Spec"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/imgdelivered/SwaggerCodegenExtension.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.14.0",
		"@types/lodash": "^4.14.170",
		"@types/parse-json": "^4.0.0",
		"@types/prettier": "^2.2.3",
		"@types/swagger-schema-official": "^2.0.21",
		"async": "^3.2.0",
		"axios": "^0.21.1",
		"commander": "^7.2.0",
		"eta": "^1.12.1",
		"js-yaml": "^4.1.0",
		"json-fixer": "^1.6.9",
		"json-parse-even-better-errors": "^2.3.1",
		"lodash": "^4.17.21",
		"make-dir": "^3.1.0",
		"nanoid": "^3.1.23",
		"node-emoji": "^1.10.0",
		"prettier": "^2.3.0",
		"run-async": "^2.4.1",
		"rxjs": "^7.1.0",
		"swagger-schema-official": "^2.0.0-bab6bed",
		"swagger2openapi": "^7.0.5",
		"ts-sync-request": "^1.4.1"
	}
}
